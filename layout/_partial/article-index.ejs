<%
    var default_cover = "http://blogimg.piaode.ren/xiangcun/img_9316.jpg"; 
%>
<div class="photo-item">
    <a href="<%- config.root %><%- item.path %>">
        <!-- <img src="<%- default_cover %>" alt=""/>         -->
        <img src="<%- item.thumbnail || default_cover %>" alt=""/>
        <div class="caption">
            <div class="work_title">
                <h1><%- item.title || "Untitled" %></h1>
            </div>
        </div>
    </a>
</div>
